<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/ffffff"

    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/button9"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:text="自己测试界面"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>


            <Button
                android:id="@+id/button1"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:text="启动小程序 有splash"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>

            <Button
                android:id="@+id/button2"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:text="启动小程序 无splash"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>

            <Button
                android:id="@+id/button3"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:text="启动小程序直达二级页面"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>

            <Button
                android:id="@+id/button4"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                android:text="延迟5秒钟关闭开启的小程序"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>

            <Button
                android:id="@+id/button5"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:text="获取已运行过的应用版本信息"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>

            <Button
                android:id="@+id/button6"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:text="原生扩展Module示例"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>

            <Button
                android:id="@+id/button7"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:text="小程序与宿主互通示例"
                android:layout_width="wrap_content"
                android:layout_height="50dp"></Button>
        </LinearLayout>

    </ScrollView>


</LinearLayout>
